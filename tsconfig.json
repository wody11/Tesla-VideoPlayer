{
  "compilerOptions": {
  "target": "ES2020",
    "module": "ESNext",
  "lib": ["DOM", "ES2020", "WebWorker"],
    "allowJs": false,
    "declaration": false,
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": "src",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false
  },
  "include": ["src/**/*"]
}
